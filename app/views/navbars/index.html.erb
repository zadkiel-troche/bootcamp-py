<doctype html>
<html>
<head>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
</head>
<body>
  <div class="container">
  <p style="color: green"><%= notice %></p>

  <h1>Navbars</h1>


  <table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Label</th>
      <th scope="col">URL</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    <% @navbars.each do |navbar| %>
      <tr>
        <th scope="row"><%= navbar.id %></th>
        <td><%= navbar.label %></td>
        <td><%= navbar.url %></td>
        <td><a href="navbars/<%= navbar.id %>" class="btn btn-warning">Ver Navbar</a></td>
      </tr>
    <% end %>
      <tr>
        <td colspan="3" class="table-active"></td>
        <td><a href="<%= new_navbar_path %>" class="btn btn-primary">Nuevo Navbar</a></td>
      </tr>
  </tbody>
</table>


  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>
</body>
</div>
</html>